script=`realpath $0` 
nortroot=`dirname $script`
nortroot=`dirname $nortroot`  
cd nortroot

echo ROOT $nortroot

rm -rf docs/nort
cp -r nort docs

rm -r docs/demo
mkdir docs/demo
cp -Lr test docs/demo/nort-full-set


rm -rf docs/dist
mkdir docs/dist
tar -cvzf docs/dist/nort.tgz nort
zip -r docs/dist/nort.zip nort/*

