script=`realpath $0` 
nortroot=`dirname $script`
nortroot=`dirname $nortroot`  
cd $nortroot

echo ROOT $nortroot

rm -rf docs/demo/nort
cp -r nort docs/demo

rm -r docs/demo/nort-full-set
cp -Lr test-app docs/demo/nort-full-set


rm -rf docs/dist
mkdir docs/dist
tar -cvzf docs/dist/nort.tgz nort
zip -r docs/dist/nort.zip nort/*

